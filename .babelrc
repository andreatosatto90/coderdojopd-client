{
  "presets": [
    ["es2015", { "modules": false }],
    "stage-0",
    "react"
  ],
  "plugins": [
    "babel-plugin-transform-decorators-legacy",
    ["module-resolver", { "alias": { "src": "./src" } }]
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
      "plugins": []
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
